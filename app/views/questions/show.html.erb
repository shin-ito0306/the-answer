<%= @question.user.name %>
<%= @question.title %>
<%= attachment_image_tag @question, :question_image %>
<%= @question.content %>
<% if @question.user.id == current_user.id %>
  <%= link_to "削除", question_path(@question.id), method: :delete %>
  <%= link_to "編集", edit_question_path(@question.id) %>
<% end %>